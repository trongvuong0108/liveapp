<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
        <shape >
            <solid android:color="#CA6E6464"/>
            <corners android:radius="15dp" />
        </shape>
    </item>
    <item
        android:left="0dp"
        android:right="0dp"
        android:top="0dp"
        android:bottom="2dp">
        <shape >
            <solid android:color="#E8F0F6"/>
            <corners android:radius="15dp" />
        </shape>
    </item>
</selector>